<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.happyme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="HappyApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HomePage" />
        <activity android:name=".MantrasActivity" />
        <activity android:name=".FamilyTimeActivity" />
        <activity android:name=".MeTimeActivity" />
        <activity android:name=".HappyTimeActivity" />
        <activity android:name=".BoostUpTimeActivity" />
        <activity android:name=".EnjoyImagesActivity" />
        <activity android:name=".GuessImagesActivity" />
    </application>

</manifest>